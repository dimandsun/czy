            </dependency>
            <dependency>
                <groupId>com.czy</groupId>
                <artifactId>fishGame</artifactId>
                <version>${czy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.czy</groupId>
                <artifactId>fx</artifactId>
                <version>${czy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.czy</groupId>
